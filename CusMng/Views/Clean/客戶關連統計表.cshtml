@model IEnumerable<CusMng.Models.vw客戶關聯資料統計表>

@{
    ViewBag.Title = "客戶關連統計表";
}
@section breadcrumb{
    <a class="navbar-brand" href="@Url.Action("客戶關連統計表","Clean")">客戶關連統計表</a>
}
<!-- Set your background image for this header on the line below. -->
<header class="intro-header" style="background-image: url('/img/home-bg.jpg')">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <div class="page-heading">
                    <h1>客戶關連統計表</h1>
                    <hr class="small">
                    @*<span class="subheading">This is what I do.</span>*@
                </div>
            </div>
        </div>
    </div>
</header>
<!-- Main Content -->
<div class="container">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.客戶名稱)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.聯絡人數量)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.帳戶數量)
                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.客戶名稱)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.聯絡人數量)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.帳戶數量)
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>
<hr>

